
var int int_value;
var double double_value;

int_value = 1;
double_value = 1.1;
int_value = (int) double_value;
print("int_value=", int_value, ", double_value=", double_value, "\n");


int_value = 1;
double_value = 1.1;
double_value = (double) int_value;
print("int_value=", int_value, ", double_value=", double_value, "\n");